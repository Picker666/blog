html, body
  background-color: #fffacf;

#app
  // user-select: none;

  strong
    font-size: 20px;
    color: lightskyblue;
    margin: 0 5px;

  p>code,li>code
    color: #0078e0;

  .navbar
    box-shadow: 0px -10px 16px #edf0f7;
    border-bottom: none;
    background-color: #001529;
    .nav-links 
      .dropdown-wrapper
        .nav-dropdown 
          .dropdown-item
            color: #000;
      a.router-link-active, a:hover
        color: #3eaf7c;
      .dropdown-wrapper
        .dropdown-title
          color: #fff;
    .site-name
      color: #fff;

    .links
      background-color: transparent;
      color: #fff;

    .logo
      border-radius: 50%;
.home
    .hero
      img
        border-radius: 50%;
        max-height: 220px;

.sidebar
  background-color: #001529;

  a.sidebar-link
   color: #fff;